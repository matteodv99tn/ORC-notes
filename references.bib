url = {http://arxiv.org/abs/2008.03596},
volume = {1},
year = {2020}
}
@article{Allouche2014,
abstract = {Proteins are chains of simple molecules called amino acids. The three-dimensional shape of a protein and its amino acid composition define its biological function. Over millions of years, living organisms have evolved a large catalog of proteins. By exploring the space of possible amino acid sequences, protein engineering aims at similarly designing tailored proteins with specific desirable properties. In Computational Protein Design (CPD), the challenge of identifying a protein that performs a given task is defined as the combinatorial optimization of a complex energy function over amino acid sequences. In this paper, we introduce the CPD problem and some of the main approaches that have been used by structural biologists to solve it, with an emphasis on the exact method embodied in the dead-end elimination/{\ss} algorithm (DEE/{\ss}). The CPD problem is a specific form of binary Cost Function Network (CFN, aka Weighted CSP). We show how DEE algorithms can be incorporated and suitably modified to be maintained during search, at reasonable computational cost. We then evaluate the efficiency of CFN algorithms as implemented in our solver toulbar2, on a set of real CPD instances built in collaboration with structural biologists. The CPD problem can be easily reduced to 0/1 Linear Programming, 0/1 Quadratic Programming, 0/1 Quadratic Optimization, Weighted Partial MaxSAT and Graphical Model optimization problems. We compare toulbar2 with these different approaches using a variety of solvers. We observe tremendous differences in the difficulty that each approach has on these instances. Overall, the CFN approach shows the best efficiency on these problems, improving by several orders of magnitude against the exact DEE/{\ss} approach. The introduction of dead-end elimination before or during search allows to further improve these results. {\textcopyright} 2014 Elsevier B.V.},
author = {Allouche, David and Andr{\'{e}}, Isabelle and Barbe, Sophie and Davies, Jessica and {De Givry}, Simon and Katsirelos, George and O'Sullivan, Barry and Prestwich, Steve and Schiex, Thomas and Traor{\'{e}}, Seydou},
doi = {10.1016/j.artint.2014.03.005},
file = {:Users/adelprete/Documents/Mendeley Desktop/Allouche et al/2014/Allouche et al. - 2014 - Computational protein design as an optimization problem.pdf:pdf},
issn = {00043702},
journal = {Artificial Intelligence},
keywords = {Bioinformatics,Computational protein design,Constraint optimization,Cost function networks,Graphical model,Integer linear programming,Maximum a posteriori inference,Maximum satisfiability,Neighborhood substitutability,Quadratic programming,Soft constraints,Weighted constraint satisfaction problem},
number = {1},
pages = {59--79},
publisher = {Elsevier B.V.},
title = {{Computational protein design as an optimization problem}},
url = {http://dx.doi.org/10.1016/j.artint.2014.03.005},
volume = {212},
year = {2014}
}
