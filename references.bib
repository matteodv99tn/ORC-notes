author = {W{\"{u}}thrich, Manuel and Widmaier, Felix and Grimminger, Felix and Akpo, Joel and Joshi, Shruti and Agrawal, Vaibhav and Hammoud, Bilal and Khadiv, Majid and Bogdanovic, Miroslav and Berenz, Vincent and Viereck, Julian and Naveau, Maximilien and Righetti, Ludovic and Sch{\"{o}}lkopf, Bernhard and Bauer, Stefan},
