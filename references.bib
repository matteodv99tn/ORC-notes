file = {:Users/adelprete/Documents/Mendeley Desktop/Mordatch, Kendall Lowrey, Galen Andrew, Zoran Popovic/2015/Mordatch, Kendall Lowrey, Galen Andrew, Zoran Popovic - 2015 - Interactive Control of Diverse Complex Characters with Neural Networks.pdf:pdf},
issn = {10495258},
