pages = {32},
pmid = {22275550},
title = {{An efficient orientation filter for inertial and inertial/magnetic sensor arrays}},
url = {http://sharenet-wii-motion-trac.googlecode.com/files/An_efficient_orientation_filter_for_inertial_and_inertialmagnetic_sensor_arrays.pdf},
year = {2010}
}
