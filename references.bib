pages = {127},
title = {{Agile quadrotor maneuvering using tensor-decomposition-based globally optimal control and onboard visual-inertial estimation}},
url = {http://hdl.handle.net/1721.1/106777},
year = {2016}
}
@article{Barasuol2013,
