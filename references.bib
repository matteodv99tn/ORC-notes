doi = {10.1109/ROBOT.2004.1308858},
file = {:Users/adelprete/Documents/Mendeley Desktop/Goswami, Kallem/2004/Goswami, Kallem - 2004 - Rate of change of angular momentum and balance maintenance of biped robots.pdf:pdf},
