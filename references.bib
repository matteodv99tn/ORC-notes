issn = {2331-8422},
keywords = {benchmarks,meta-learning,multi-task reinforcement learning},
number = {CoRL},
pages = {1--17},
title = {{Meta-World: A Benchmark and Evaluation for Multi-Task and Meta Reinforcement Learning}},
url = {http://arxiv.org/abs/1910.10897},
