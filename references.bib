file = {:Users/adelprete/Documents/Mendeley Desktop/Mastalli et al/2020/Mastalli et al. - 2020 - Crocoddyl An Efficient and Versatile Framework for Multi-Contact Optimal Control.pdf:pdf},
isbn = {9781728173955},
issn = {10504729},
pages = {2536--2542},
title = {{Crocoddyl: An Efficient and Versatile Framework for Multi-Contact Optimal Control}},
