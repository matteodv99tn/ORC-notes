journal = {Proceedings - IEEE International Conference on Robotics and Automation},
pages = {1794--1801},
publisher = {IEEE},
title = {{Realizing dynamic and efficient bipedal locomotion on the humanoid robot DURUS}},
volume = {2016-June},
year = {2016}
}
